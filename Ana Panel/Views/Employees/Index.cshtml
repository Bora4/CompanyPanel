@model List<Ana_Panel.Models.Domain.Employee>

<h1>Personel Listesi</h1>

<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Isim</th>
			<th>Email</th>
			<th>Maas</th>
			<th>Dogum Tarihi</th>
			<th>Departman</th>
			<th>Unvan</th>
			<th> </th>
		</tr>
	</thead>
	<tbody>
		@foreach (var employee in Model)
		{
			<tr>
				<td>@employee.Id</td>
				<td>@employee.Name</td>
				<td>@employee.Email</td>
				<td>@employee.Salary</td>
				<td>@employee.DateOfBirth.ToString("dd/MM/yyyy")</td>
				<td>@employee.Department</td>
				<td>@employee.Title</td>
				<td><a href = "Employees/View/@employee.Id">Goruntule</a></td>
			</tr>
		}
	</tbody>
</table>